<div class="jumbotron">
<h1>WOWOWOWOOWOWOWOW</h1>
<h2>HUEHUEHEUHEUHEUHEU</h2>
</div>
<div class="center">
<div class="row">
  <% @listings.each do |listing| %>
    <div class="col-md-3">
      <div class="thumbnail">
        <%= link_to image_tag(listing.image.url), listing %>
      <div class="caption">
        <h3><%= listing.name %></h3>
        <p><%= listing.price %></p>
        <p><%= listing.user.name%></p>
      </div>
      </div>
    </div>
  <% end %> 
</div>
</div>

<br>

<% if user_signed_in? %>
<%= link_to new_listing_path, class: "btn btn-success", data: { no_turbolink: true } do %>
  <i class="glyphicon glyphicon-heart"></i> New Listing
<% end %>

<% end %>